.out{
  margin: auto;
  margin-top: 20rpx;
  width: 710rpx;
  height: 150rpx;
  border-radius: 10rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-around;
}
.count{
  width: 660rpx;
  height: 35rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.effectTime{
  font-size: 32rpx;
  color: #888888;
}

.effectCount{
  font-size: 30rpx;
  color: black;
}

.invalid{
  font-size: 30rpx;
  color: #888888;
}