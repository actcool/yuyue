page{
  background-color: #F6F6F6;
}
.title{
  width: 100%;
  height: 360rpx;
  background-color: #3BA6D2;
  display: flex;
  justify-content: center;
}
.welcome{
  position: relative;
  top: 65rpx;
  font-size: 40rpx;
  color: #F6F6F6;
}

.form{
  position: relative;
  bottom: 122rpx;
}

.white{
  background-color: white;
  width: 710rpx;
  margin: auto;
  height: 414rpx;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.remind{
  margin-top: 20rpx;
  height: 29rpx;
  font-size: 24rpx;
  line-height: 34rpx;
}

.time{
  width: 650rpx;
  height: 74rpx;
  background-color: #F6F6F6;
  border-radius: 10rpx;
  margin-top: 31rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.cannot{
  color: #AAAAAA;
}
.left{
  margin-left: 30rpx;
}
.right{
  margin-right: 30rpx;
}
.left_input{
  margin-left: 30rpx;
  width: 250rpx;
  height: 74rpx;
  display: flex;
  align-items: center;
}
.right_input{
  margin-right: 30rpx;
  width: 300rpx;
  height: 74rpx;
  display: flex;
  align-items: center;
  flex-direction: row-reverse;
}

.submit{
  width: 710rpx;
  height: 84rpx;
  border-radius: 42rpx;
  margin: auto;
  margin-top: 60rpx;
  background-color: #3BA6D2;
  font-size: 30rpx;
  color: white;
  display: flex;
  align-items: center;
  justify-content: center;
}
.cancel{
  width: 710rpx;
  height: 84rpx;
  border-radius: 42rpx;
  margin: auto;
  margin-top: 50rpx;
  background-color:white ;
  font-size: 30rpx;
  color: #3BA6D2;
  display: flex;
  align-items: center;
  justify-content: center;
}


.not{
  color: #AAAAAA;
}

.cmp-bc{
  position: absolute;
  top:0rpx;
  width: 100%;
  height: 1500rpx;
  background-color: rgba(0, 0, 0, 0.5);
